{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./src",
    "removeComments": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowUnreachableCode":false,
    "paths": {
      "@shared/*": ["./src/shared/*"],
    }
  }
}
